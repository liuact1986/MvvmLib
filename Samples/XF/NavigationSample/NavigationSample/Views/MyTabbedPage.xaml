<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NavigationSample.Views.MyTabbedPage"
             xmlns:nav="clr-namespace:MvvmLib.Navigation;assembly=MvvmLib.XF"
             nav:ViewModelLocator.ResolveViewModel="True"
             Title="My Tabbed Page"
             xmlns:views="using:NavigationSample.Views">

    <ContentPage Title="Tab A">
        <Label Text="Tab A"></Label>
    </ContentPage>

    <NavigationPage Title="Tab B" x:Name="tabNav" x:FieldModifier="Public">
        <x:Arguments>
            <views:ChildPageA/>
        </x:Arguments>
    </NavigationPage>
    
</TabbedPage>